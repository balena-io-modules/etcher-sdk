    "lint": "balena-lint lib typings examples",
    "lint-fix": "balena-lint --fix lib typings examples tests",
    "build": "tsc",
    "doc": "typedoc --readme none --plugin typedoc-plugin-markdown --out doc lib && npm run sed",
